@model IEnumerable<WebApplication1.Models.tipoexame>

@{
    ViewBag.Title = "Index";
}

<h2>Tipos de Exames</h2>

<p>
    @Html.ActionLink("Cadastrar Novo Tipo de Exame", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.nome)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.descricao)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.nome)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.descricao)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.id_tipoexame }) |
            @Html.ActionLink("Detalhes", "Details", new { id=item.id_tipoexame }) |
            @Html.ActionLink("Deletar", "Delete", new { id=item.id_tipoexame })
        </td>
    </tr>
}

</table>
